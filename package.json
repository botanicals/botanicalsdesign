{
  "name": "botanicals-design",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "cms": "npx netlify-cms-proxy-server",
    "dev:cms": "concurrently \"npm run dev\" \"npm run cms\"",
    "noIndex": "next build && echo 'User-agent: *\nDisallow: /' > public/robots.txt",
    "build:admin-css": "npx tailwindcss build styles/globals.css -o public/admin/styles.css",
    "build:admin-scss": "sass styles/main.scss public/admin/main.css",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "npm run build:admin-css && npm run build:admin-scss && npm run build && next export"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "axios": "^0.26.0",
    "cloudinary": "^1.28.1",
    "frontmatter-markdown-loader": "^3.6.3",
    "netlify-cms-app": "^2.15.65",
    "next": "12.0.10",
    "next-seo": "^5.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "^8.0.0",
    "rehype-slug": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "sass": "^1.49.8"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.39",
    "@types/webpack-env": "^1.16.3",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.18",
    "typescript": "^4.5.5"
  }
}
